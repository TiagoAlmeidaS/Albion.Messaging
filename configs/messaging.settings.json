{
  "rabbitMq": {
    "uri": "amqp://myuser:mysecurepassword@tastechdeveloper.shop:5672",
    "dispatchConsumersAsync": true
  },
  "exchanges": [
    { "name": "albion.sniffer", "type": "topic", "durable": true, "autoDelete": false },
    { "name": "albion.fishing", "type": "direct", "durable": true, "autoDelete": false },
    { "name": "albion.events.raw", "type": "fanout", "durable": true, "autoDelete": false },
    { "name": "albion.clusters", "type": "topic", "durable": true, "autoDelete": false },
    { "name": "albion.players", "type": "topic", "durable": true, "autoDelete": false },
    { "name": "albion.mobs", "type": "topic", "durable": true, "autoDelete": false },
    { "name": "albion.resources", "type": "topic", "durable": true, "autoDelete": false },
    { "name": "albion.dungeons", "type": "topic", "durable": true, "autoDelete": false },
    { "name": "albion.wisps", "type": "topic", "durable": true, "autoDelete": false },
    { "name": "albion.chests", "type": "topic", "durable": true, "autoDelete": false },
    { "name": "albion.operations", "type": "topic", "durable": true, "autoDelete": false }
  ],
  "queues": [
    { "name": "sniffer.raw.packets", "bindingKey": "albion.packet.*", "exchange": "albion.sniffer", "durable": true },  
    { "name": "clusters.change", "bindingKey": "albion.event.changecluster", "exchange": "albion.sniffer", "durable": true },
    { "name": "clusters.loadobjects", "bindingKey": "albion.event.loadclusterobjects", "exchange": "albion.sniffer", "durable": true },
    { "name": "clusters.mistsplayer", "bindingKey": "albion.event.mistsplayerjoinedinfo", "exchange": "albion.sniffer", "durable": true },
    { "name": "players.newcharacter", "bindingKey": "albion.event.newcharacter", "exchange": "albion.sniffer", "durable": true },
    { "name": "players.equipment", "bindingKey": "albion.event.characterequipmentchanged", "exchange": "albion.sniffer", "durable": true },
    { "name": "players.health", "bindingKey": "albion.event.healthupdate", "exchange": "albion.sniffer", "durable": true },
    { "name": "players.regeneration", "bindingKey": "albion.event.regenerationchanged", "exchange": "albion.sniffer", "durable": true },
    { "name": "players.mounted", "bindingKey": "albion.event.mounted", "exchange": "albion.sniffer", "durable": true },
    { "name": "players.move", "bindingKey": "albion.event.move", "exchange": "albion.sniffer", "durable": true },
    { "name": "players.flagging", "bindingKey": "albion.event.changeflaggingfinished", "exchange": "albion.sniffer", "durable": true },
    { "name": "mobs.new", "bindingKey": "albion.event.newmob", "exchange": "albion.sniffer", "durable": true },
    { "name": "mobs.state", "bindingKey": "albion.event.mobchangestate", "exchange": "albion.sniffer", "durable": true },
    { "name": "resources.new", "bindingKey": "albion.event.newharvestable", "exchange": "albion.sniffer", "durable": true },
    { "name": "resources.list", "bindingKey": "albion.event.newharvestableslist", "exchange": "albion.sniffer", "durable": true },
    { "name": "resources.state", "bindingKey": "albion.event.harvestablechangestate", "exchange": "albion.sniffer", "durable": true },
    { "name": "dungeons.new", "bindingKey": "albion.event.newdungeon", "exchange": "albion.sniffer", "durable": true },
    { "name": "fishing.zone", "bindingKey": "albion.event.newfishingzone", "exchange": "albion.sniffer", "durable": true },
    { "name": "fishing.bot.trigger", "bindingKey": "fish.*", "exchange": "albion.fishing", "durable": true },
    { "name": "wisps.new", "bindingKey": "albion.event.newgatedwisp", "exchange": "albion.sniffer", "durable": true },
    { "name": "wisps.gate", "bindingKey": "albion.event.wispgateopened", "exchange": "albion.sniffer", "durable": true },
    { "name": "chests.new", "bindingKey": "albion.event.newlootchest", "exchange": "albion.sniffer", "durable": true },
    { "name": "sync.keys", "bindingKey": "albion.event.keysync", "exchange": "albion.sniffer", "durable": true },
    { "name": "objects.leave", "bindingKey": "albion.event.leave", "exchange": "albion.sniffer", "durable": true },
    { "name": "operations.join", "bindingKey": "albion.event.joinresponse", "exchange": "albion.sniffer", "durable": true },
    { "name": "operations.move", "bindingKey": "albion.event.moverequest", "exchange": "albion.sniffer", "durable": true },
    { "name": "sniffer.events.newcharacter", "bindingKey": "albion.event.newcharacter", "exchange": "albion.sniffer", "durable": true },    
    { "name": "sniffer.events.*", "bindingKey": "albion.event.*", "exchange": "albion.sniffer", "durable": true }
  ]
}
