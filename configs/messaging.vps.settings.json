{
  "rabbitMq": {
    "uri": "amqp://myuser:mysecurepassword@localhost:5672",
    "dispatchConsumersAsync": true
  },
  "exchanges": [
    { "name": "albion.sniffer", "type": "topic", "durable": true, "autoDelete": false },
    { "name": "albion.fishing", "type": "direct", "durable": true, "autoDelete": false },
    { "name": "albion.events.raw", "type": "fanout", "durable": true, "autoDelete": false },
    { "name": "albion.clusters", "type": "topic", "durable": true, "autoDelete": false },
    { "name": "albion.players", "type": "topic", "durable": true, "autoDelete": false },
    { "name": "albion.mobs", "type": "topic", "durable": true, "autoDelete": false },
    { "name": "albion.resources", "type": "topic", "durable": true, "autoDelete": false },
    { "name": "albion.dungeons", "type": "topic", "durable": true, "autoDelete": false },
    { "name": "albion.wisps", "type": "topic", "durable": true, "autoDelete": false },
    { "name": "albion.chests", "type": "topic", "durable": true, "autoDelete": false },
    { "name": "albion.operations", "type": "topic", "durable": true, "autoDelete": false }
  ],
  "queues": [
    { "name": "sniffer.raw.packets", "bindingKey": "albion.packet.*", "exchange": "albion.sniffer", "durable": true },
    { "name": "clusters.change", "bindingKey": "albion.event.cluster.changed", "exchange": "albion.sniffer", "durable": true },
    { "name": "clusters.loadobjects", "bindingKey": "albion.event.cluster.objects.loaded", "exchange": "albion.sniffer", "durable": true },
    { "name": "clusters.mistsplayer", "bindingKey": "albion.event.mists.player.joined", "exchange": "albion.sniffer", "durable": true },
    { "name": "players.newcharacter", "bindingKey": "albion.event.player.joined", "exchange": "albion.sniffer", "durable": true },
    { "name": "players.equipment", "bindingKey": "albion.event.player.equipment.changed", "exchange": "albion.sniffer", "durable": true },
    { "name": "players.health", "bindingKey": "albion.event.player.health.updated", "exchange": "albion.sniffer", "durable": true },
    { "name": "players.regeneration", "bindingKey": "albion.event.player.regeneration.changed", "exchange": "albion.sniffer", "durable": true },
    { "name": "players.mounted", "bindingKey": "albion.event.player.mounted.changed", "exchange": "albion.sniffer", "durable": true },
    { "name": "players.move", "bindingKey": "albion.event.player.moved", "exchange": "albion.sniffer", "durable": true },
    { "name": "players.flagging", "bindingKey": "albion.event.player.flagging.finished", "exchange": "albion.sniffer", "durable": true },
    { "name": "mobs.new", "bindingKey": "albion.event.mob.spawned", "exchange": "albion.sniffer", "durable": true },
    { "name": "mobs.state", "bindingKey": "albion.event.mob.state.changed", "exchange": "albion.sniffer", "durable": true },
    { "name": "resources.new", "bindingKey": "albion.event.harvestable.found", "exchange": "albion.sniffer", "durable": true },
    { "name": "resources.list", "bindingKey": "albion.event.harvestable.list.found", "exchange": "albion.sniffer", "durable": true },
    { "name": "resources.state", "bindingKey": "albion.event.harvestable.state.changed", "exchange": "albion.sniffer", "durable": true },
    { "name": "dungeons.new", "bindingKey": "albion.event.world.dungeon.found", "exchange": "albion.sniffer", "durable": true },
    { "name": "fishing.zone", "bindingKey": "albion.event.world.fishing.zone.found", "exchange": "albion.sniffer", "durable": true },
    { "name": "fishing.bot.trigger", "bindingKey": "fish.*", "exchange": "albion.fishing", "durable": true },
    { "name": "wisps.new", "bindingKey": "albion.event.world.gated.wisp.found", "exchange": "albion.sniffer", "durable": true },
    { "name": "wisps.gate", "bindingKey": "albion.event.world.wisp.gate.opened", "exchange": "albion.sniffer", "durable": true },
    { "name": "chests.new", "bindingKey": "albion.event.world.loot.chest.found", "exchange": "albion.sniffer", "durable": true },
    { "name": "sync.keys", "bindingKey": "albion.event.cluster.key.sync", "exchange": "albion.sniffer", "durable": true },
    { "name": "objects.leave", "bindingKey": "albion.event.player.left", "exchange": "albion.sniffer", "durable": true },
    { "name": "operations.join", "bindingKey": "albion.event.joinresponse", "exchange": "albion.sniffer", "durable": true },
    { "name": "operations.move", "bindingKey": "albion.event.player.move.request", "exchange": "albion.sniffer", "durable": true },
    { "name": "sniffer.events.newcharacter", "bindingKey": "albion.event.player.joined", "exchange": "albion.sniffer", "durable": true },
    { "name": "sniffer.events.*", "bindingKey": "albion.event.*", "exchange": "albion.sniffer", "durable": true }
  ]
} 